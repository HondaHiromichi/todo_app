<h1>TodoItems#index</h1>
<table>
  <thead>
    <tr>
      <th>title</th>
      <th>contents</th>
    </tr>
  </thead>
    <% @todo_items.each do |todo_item| %>
      <tr>
        <th><%= todo_item.title %></th>
        <th><%= todo_item.contents %></th>
        <td><%= link_to 'Show', todo_item %></td>
        <td><%= link_to 'Edit', edit_todo_item_path(todo_item) %></td>
        <td><%= link_to '削除', todo_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</table>
